<?xml version="1.0"?>
<configuration>
  <appSettings>
    
    <!-- _____________________________________________ -->
    <!--This is where you can configure the application-->
    <!-- _____________________________________________ -->
    
    <!-- Put you name here-->
    <add key="BattleNetUserName" value="Ghost"/>
    
    
    <!-- If you keep it blank, it will goes in your MyDocument/Stracraft II folder -->
    <add key="YourReplayFolder" value=""/>
    
    
    <!--  Display format that will be twitted
          {0} = Win or Loss
          {1} = Opponent nickname
          {2} = Map
          {3} = Your nickname
          
          Ex.: I Win against EGIdra on MapXYZ
    -->
    <add key ="DisplayString" value="I {0} against {1} on {2}"/>
    
    
    <!--Do not need to modify anything below this line, your Twitter access code will be written here-->
    <add key="AccessToken" value=""/>
    <add key="AccessTokenSecret" value=""/>
    <add key="ConsumerKey" value=""/>
    <add key="ConsumerSecret" value=""/>

  </appSettings>
  
<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>

</startup></configuration>
